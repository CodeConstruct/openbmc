[Unit]
Description=Phosphor Fan Control Daemon
Requires=xyz.openbmc_project.Hwmon@-i2c{ADDR}-max31785\x4052.service
After=xyz.openbmc_project.Hwmon@-i2c{ADDR}-max31785\x4052.service
Requires=phosphor-cooling-type@%i.service
After=phosphor-cooling-type@%i.service

[Service]
Restart=on-failure
ExecStart=/usr/bin/phosphor-fan-control --control

[Install]
RequiredBy=obmc-chassis-poweron@%i.target
